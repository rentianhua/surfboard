<?xml version="1.0" encoding="utf-8"?>
<iConnect.sessionManagement defaultProvider="DbProvider" defaultScavenger="backgroundScavenger">
  <providers>
    <add name="DbProvider" type="Smartac.SR.SessionManagement.DbSessionProvider, Smartac.SR.SessionManagement" database="appDatabase" applicationName="app" maxConcurrentSessions="10" thresholdBreakingBehavior="BlockNewAttempt" timeout="00:30:00" renewalRefreshInterval="00:01:00" />
  </providers>
  <scavengers>
    <add name="backgroundScavenger" type="Smartac.SR.SessionManagement.BackgroundScavenger, Smartac.SR.SessionManagement" database="appDatabase" applicationName="app" scavengingWorkInterval="00:30:00" scavengingWindow="01:00:00" />
  </scavengers>
</iConnect.sessionManagement>