<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration"/>
    <!--<section name="cachingConfiguration" type="Microsoft.Practices.EnterpriseLibrary.Caching.Configuration.CacheManagerSettings, Microsoft.Practices.EnterpriseLibrary.Caching" />
    <section name="dataConfiguration" type="Microsoft.Practices.EnterpriseLibrary.Data.Configuration.DatabaseSettings, Microsoft.Practices.EnterpriseLibrary.Data" />-->
    <section name="sr.settingSource" type="Cedar.Core.SettingSource.Configuration.SettingSourceSettings,Cedar.Core"/>
    <section name="sr.serviceLocation" type="Cedar.Core.IoC.Configuration.ServiceLocationSettings,Cedar.Core"/>
    <section name="sr.applicationContexts" type="Cedar.Core.ApplicationContexts.Configuration.ApplicationContextSettings, Cedar.Core"/>
    <!--<section name="SimpleFileSettingSource" type="Smartac.SR.Core.SettingSource.SimpleFileSettingSource,Smartac.SR.Core"/>-->
  </configSections>
  <system.data>
    <DbProviderFactories>
      <remove invariant="MySql.Data.MySqlClient"/>
      <add name="MySQL Data Provider" invariant="MySql.Data.MySqlClient" description=".Net Framework Data Provider for MySQL" type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data, Version=6.9.7.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d"/>
    </DbProviderFactories>
  </system.data>
  <connectionStrings configSource="ConfigFiles\connectionStrings.config"/>
  <!--<sr.settingSource configSource="ConfigFiles\settingSource.config" />-->
  <!--<sr.serviceLocation filePath="ConfigFiles\serviceLocation.config" />-->
  <!--sr.settingSource-->
  <sr.settingSource defaultSettingSource="srdefaultSettingSource">
    <settingSources>
      <add name="srdefaultSettingSource" type="Cedar.Core.EntLib.SettingSource.SimpleFileSettingSource,Cedar.Core.EntLib"/>
    </settingSources>
  </sr.settingSource>
  <!--sr.serviceLocation-->
  <sr.serviceLocation defaultServiceLocator="srdefaultServiceLocator">
    <serviceLocators>
      <add name="srdefaultServiceLocator" type="Cedar.Core.EntLib.IoC.UnityContainerServiceLocator,Cedar.Core.EntLib"/>
    </serviceLocators>
    <resolvedAssemblies>
      <add assembly="Cedar.Core"/>
      <add assembly="Cedar.Core.EntLib"/>
    </resolvedAssemblies>
  </sr.serviceLocation>
  <unity>
    <sectionExtension type="Microsoft.Practices.Unity.InterceptionExtension.Configuration.InterceptionConfigurationExtension, Microsoft.Practices.Unity.Interception.Configuration"/>
    <sectionExtension type="Cedar.Core.EntLib.IoC.Configuration.AutoInterceptionConfigurationExtension, Cedar.Core.EntLib"/>
    <container>
      <register type="Smartac.SR.Modules.Customer.Interface.ICustomerManagementService, Smartac.SR.Modules.Customer.Interface" mapTo="Smartac.SR.Modules.Customer.BusinessService.CustomerManagementService, Smartac.SR.Modules.Customer.BusinessService"/>
      <autoInterception>
        <interceptor type="TransparentProxyInterceptor"/>
      </autoInterception>
      <extension type="Interception"/>
      <extension type="AutoInterception"/>
      <!--<extendedInterception interceptor="Microsoft.Practices.Unity.InterceptionExtension.TransparentProxyInterceptor,Microsoft.Practices.Unity.Interception"/>-->
    </container>
  </unity>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>
  </appSettings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.6"/>
    <httpRuntime targetFramework="4.5"/>
  </system.web>
  <system.webServer>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0"/>
      <remove name="OPTIONSVerbHandler"/>
      <remove name="TRACEVerbHandler"/>
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0"/>
    </handlers>
  </system.webServer>
  <runtime>
