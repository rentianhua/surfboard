using System;

namespace HiiP.Framework.Logging.Interface.ValidationEntity
{
    [Serializable]
    public class DateTimeCompare
    {
        public DateTimeCompare()
        {
 
        }

        public DateTimeCompare(DateTime startTime, DateTime endTime)
        {
            StartTime = startTime;
            EndTime = endTime;
        }

        public DateTime StartTime
        {
            get;
            set;
        }

        public DateTime EndTime
        {
            get;
            set;
        }
    }
}